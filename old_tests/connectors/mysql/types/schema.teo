connector {
  provider .mysql
  url "mysql://127.0.0.1:3307/test_connectors_mysql_types"
}

server {
  bind ("0.0.0.0", 4012)
}

enum Sex {
  MALE
  FEMALE
}

model Support {
  @id @autoIncrement @readonly
  id: Int
  int32: Int32?
  int64: Int64?
  float32: Float32?
  float64: Float64?
  bool: Bool?
  string: String?
  date: Date?
  dateTime: DateTime?
  decimal: Decimal?
  sex: Sex?
}
